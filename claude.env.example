# Claude AI 集成配置
# 复制此文件为 claude.env 并填入实际值

# Claude API 配置
CLAUDE_API_KEY=your_claude_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
CLAUDE_MODEL=claude-3-sonnet-20240229

# GitHub 集成配置
GITHUB_TOKEN=your_github_token_here
GITHUB_WEBHOOK_SECRET=your_webhook_secret_here

# 项目配置
PROJECT_NAME="索克生活平台"
PROJECT_DESCRIPTION="智能健康管理平台"
PROJECT_LANGUAGE="TypeScript,Python,React Native"

# 通知配置
SLACK_WEBHOOK_URL=your_slack_webhook_url_here
EMAIL_ALERTS_ENABLED=true
NOTIFICATION_CHANNEL=#suoke-life-dev

# 代码审查配置
AUTO_APPROVE_MINOR=false
REQUIRE_HUMAN_REVIEW=true
MIN_REVIEW_SCORE=80

# 安全配置
SECURITY_SCAN_ENABLED=true
VULNERABILITY_THRESHOLD=medium
COMPLIANCE_CHECK_ENABLED=true

# 性能配置
PERFORMANCE_MONITORING=true
RESPONSE_TIME_THRESHOLD=100
MEMORY_USAGE_THRESHOLD=80

# 文档生成配置
AUTO_GENERATE_DOCS=true
DOC_LANGUAGES=zh-CN,en-US
INCLUDE_API_DOCS=true
INCLUDE_ARCHITECTURE_DOCS=true

# 医疗AI特定配置
MEDICAL_COMPLIANCE_CHECK=true
HEALTH_DATA_PRIVACY_SCAN=true
TCM_KNOWLEDGE_VALIDATION=true
AI_AGENT_COLLABORATION_CHECK=true

# 调试配置
DEBUG_MODE=false
VERBOSE_LOGGING=false
LOG_LEVEL=info 