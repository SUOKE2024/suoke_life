var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _Button = _interopRequireDefault(require("../../../components/ui/Button"));
var _jsxRuntime = require("react/jsx-runtime");
describe('Button Component', function () {
  it('renders correctly with title', function () {
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Button.default, {
        title: "\u6D4B\u8BD5\u6309\u94AE"
      })),
      getByText = _render.getByText;
    expect(getByText('测试按钮')).toBeTruthy();
  });
  it('calls onPress when pressed', function () {
    var mockOnPress = jest.fn();
    var _render2 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Button.default, {
        title: "\u70B9\u51FB\u6309\u94AE",
        onPress: mockOnPress
      })),
      getByText = _render2.getByText;
    _reactNative.fireEvent.press(getByText('点击按钮'));
    expect(mockOnPress).toHaveBeenCalledTimes(1);
  });
  it('does not call onPress when disabled', function () {
    var mockOnPress = jest.fn();
    var _render3 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Button.default, {
        title: "\u7981\u7528\u6309\u94AE",
        onPress: mockOnPress,
        disabled: true
      })),
      getByText = _render3.getByText;
    _reactNative.fireEvent.press(getByText('禁用按钮'));
    expect(mockOnPress).not.toHaveBeenCalled();
  });
  it('shows loading indicator when loading', function () {
    var _render4 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Button.default, {
        title: "\u52A0\u8F7D\u6309\u94AE",
        loading: true,
        testID: "loading-button"
      })),
      getByTestId = _render4.getByTestId;
    expect(getByTestId('loading-button')).toBeTruthy();
  });
  it('applies correct variant styles', function () {
    var _render5 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Button.default, {
        title: "\u4E3B\u8981\u6309\u94AE",
        variant: "primary",
        testID: "primary-button"
      })),
      getByTestId = _render5.getByTestId;
    expect(getByTestId('primary-button')).toBeTruthy();
  });
  it('applies correct size styles', function () {
    var _render6 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Button.default, {
        title: "\u5927\u6309\u94AE",
        size: "large",
        testID: "large-button"
      })),
      getByTestId = _render6.getByTestId;
    expect(getByTestId('large-button')).toBeTruthy();
  });
});
