var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _Switch = _interopRequireDefault(require("../../../components/ui/Switch"));
var _jsxRuntime = require("react/jsx-runtime");
describe('Switch组件', function () {
  it('应该正确渲染', function () {
    var mockOnValueChange = jest.fn();
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Switch.default, {
        value: false,
        onValueChange: mockOnValueChange,
        testID: "test-switch"
      })),
      getByTestId = _render.getByTestId;
    expect(getByTestId('test-switch')).toBeTruthy();
  });
  it('应该显示标签', function () {
    var mockOnValueChange = jest.fn();
    var _render2 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Switch.default, {
        value: false,
        onValueChange: mockOnValueChange,
        label: "\u6D4B\u8BD5\u6807\u7B7E"
      })),
      getByText = _render2.getByText;
    expect(getByText('测试标签')).toBeTruthy();
  });
  it('应该显示描述', function () {
    var mockOnValueChange = jest.fn();
    var _render3 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Switch.default, {
        value: false,
        onValueChange: mockOnValueChange,
        description: "\u6D4B\u8BD5\u63CF\u8FF0"
      })),
      getByText = _render3.getByText;
    expect(getByText('测试描述')).toBeTruthy();
  });
  it('应该处理值变化', function () {
    var mockOnValueChange = jest.fn();
    var _render4 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Switch.default, {
        value: false,
        onValueChange: mockOnValueChange,
        testID: "test-switch"
      })),
      getByTestId = _render4.getByTestId;
    (0, _reactNative.fireEvent)(getByTestId('test-switch'), 'valueChange', true);
    expect(mockOnValueChange).toHaveBeenCalledWith(true);
  });
  it('应该正确渲染禁用状态', function () {
    var mockOnValueChange = jest.fn();
    var _render5 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Switch.default, {
        value: false,
        onValueChange: mockOnValueChange,
        disabled: true,
        testID: "test-switch"
      })),
      getByTestId = _render5.getByTestId;
    expect(getByTestId('test-switch')).toBeTruthy();
  });
});
