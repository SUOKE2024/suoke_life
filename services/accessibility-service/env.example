# accessibility-service 环境变量配置

# 服务配置
SERVICE_NAME=accessibility-service
SERVICE_VERSION=1.0.0
DEBUG=true
LOG_LEVEL=INFO

# 数据库配置
DATABASE_URL=postgresql://user:password@localhost:5432/accessibility_service_db

# Redis配置
REDIS_URL=redis://localhost:6379/0

# API配置
API_HOST=0.0.0.0
API_PORT=8000

# 安全配置
SECRET_KEY=your-secret-key-here
JWT_ALGORITHM=HS256
JWT_EXPIRE_MINUTES=30

# 监控配置
ENABLE_METRICS=true
METRICS_PORT=9090
