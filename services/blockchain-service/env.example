# 应用配置
APP_NAME="SuoKe Blockchain Service"
APP_VERSION="0.1.0"
DEBUG=false
ENVIRONMENT=development

# 数据库配置
DATABASE__HOST=localhost
DATABASE__PORT=5432
DATABASE__USER=postgres
DATABASE__PASSWORD=your_password_here
DATABASE__DATABASE=blockchain_service
DATABASE__POOL_SIZE=10
DATABASE__MAX_OVERFLOW=20

# Redis 配置
REDIS__HOST=localhost
REDIS__PORT=6379
REDIS__PASSWORD=
REDIS__DATABASE=0
REDIS__MAX_CONNECTIONS=20

# 区块链配置
BLOCKCHAIN__ETH_NODE_URL=http://localhost:8545
BLOCKCHAIN__CHAIN_ID=1337
BLOCKCHAIN__DEPLOYER_PRIVATE_KEY=your_private_key_here
BLOCKCHAIN__GAS_LIMIT=6000000
BLOCKCHAIN__GAS_PRICE=20000000000

# 智能合约地址（部署后填写）
BLOCKCHAIN__HEALTH_DATA_STORAGE_ADDRESS=
BLOCKCHAIN__ZKP_VERIFIER_ADDRESS=
BLOCKCHAIN__ACCESS_CONTROL_ADDRESS=

# gRPC 配置
GRPC__HOST=0.0.0.0
GRPC__PORT=50055
GRPC__MAX_WORKERS=10

# 安全配置
SECURITY__JWT_SECRET_KEY=your-super-secret-jwt-key-change-in-production
SECURITY__JWT_ALGORITHM=HS256
SECURITY__JWT_EXPIRATION_HOURS=24
SECURITY__ENCRYPTION_KEY=your-encryption-key-here

# 监控配置
MONITORING__ENABLE_METRICS=true
MONITORING__METRICS_PORT=9090
MONITORING__ENABLE_TRACING=true
MONITORING__JAEGER_ENDPOINT=http://localhost:14268/api/traces
MONITORING__LOG_LEVEL=INFO
MONITORING__LOG_FORMAT=json 