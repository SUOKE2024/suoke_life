# Look Service Environment Configuration

# Service Settings
SERVICE_NAME=look-service
SERVICE_VERSION=1.0.0
ENVIRONMENT=development
DEBUG=true
TESTING=false

# Server Settings
HOST=0.0.0.0
PORT=8080
WORKERS=1

# gRPC Settings
GRPC_HOST=0.0.0.0
GRPC_PORT=50051
GRPC_MAX_WORKERS=10

# Security
SECRET_KEY=your-secret-key-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS Settings
CORS_ORIGINS=["*"]
CORS_METHODS=["*"]
CORS_HEADERS=["*"]

# Database Settings - PostgreSQL
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=look_service

# Database Settings - Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Database Settings - MongoDB
MONGO_HOST=localhost
MONGO_PORT=27017
MONGO_USER=
MONGO_PASSWORD=
MONGO_DB=look_service

# Database Settings - Milvus
MILVUS_HOST=localhost
MILVUS_PORT=19530
MILVUS_USER=
MILVUS_PASSWORD=

# Machine Learning Settings
FACE_MODEL_PATH=models/face_detection.onnx
TONGUE_MODEL_PATH=models/tongue_analysis.onnx
EYE_MODEL_PATH=models/eye_analysis.onnx
MAX_IMAGE_SIZE=1024
BATCH_SIZE=8
CONFIDENCE_THRESHOLD=0.7
USE_GPU=false
GPU_DEVICE_ID=0
FEATURE_DIM=512
SIMILARITY_THRESHOLD=0.8

# Monitoring Settings
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_FILE=logs/look-service.log
ENABLE_METRICS=true
METRICS_PORT=9090
ENABLE_TRACING=false
JAEGER_ENDPOINT=
HEALTH_CHECK_INTERVAL=30
CIRCUIT_BREAKER_FAILURE_THRESHOLD=5
CIRCUIT_BREAKER_RECOVERY_TIMEOUT=60

# File Storage Settings
UPLOAD_DIR=uploads
MAX_FILE_SIZE=10485760  # 10MB
ALLOWED_EXTENSIONS=["jpg", "jpeg", "png", "bmp", "tiff"] 