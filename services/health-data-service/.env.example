# 索克生活健康数据服务环境变量示例
# 复制此文件为.env并根据需要修改值

# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_NAME=health_data

# 服务器配置
REST_PORT=8080
GRPC_PORT=50051
LOG_LEVEL=INFO

# 区块链服务
BLOCKCHAIN_ENABLED=false
BLOCKCHAIN_SERVICE_URL=http://localhost:8081

# 安全配置
JWT_SECRET=your-secret-key-change-me-in-production

# 缓存配置
CACHE_ENABLED=false
REDIS_URL=redis://localhost:6379/0

# 消息队列配置
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest

# 中医体质模型配置路径
TCM_MODEL_PATH=config/models/tcm_constitution_model.pkl
TCM_FEATURE_WEIGHTS_PATH=config/models/tcm_feature_weights.json